<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.starvelater">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".helperClasses.CustomGridLayout"></activity>
        <activity android:name=".Custom_Gridlayout" />
        <activity android:name=".RestaurantProfile" />
        <activity android:name=".common.loginsignup.StartUpScreen" />
        <activity
            android:name=".common.loginsignup.SignUpActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".common.loginsignup.LoginActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".user.AllCategories" />
        <activity android:name=".common.OnBoarding" />
        <activity android:name=".common.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".user.UserDashboard" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>